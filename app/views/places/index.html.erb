<% @places.each do |place| %>
  <div class="booyah-box col-10 offset-1">
    <h1><%= place.name %></h1>
    <i><%= place.address %></i><br /><br />
    <p><%= place.description %></p><br />
  </div>

<% end %>
<div class="col-10 offset-1" id="pagination-nav">
  <%= link_to_prev_page @places, 'Previous Page' %>
  <span class="float-right">
    <%= link_to_next_page @places, 'Next Page' %>
  </span>
</div>